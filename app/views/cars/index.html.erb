<p id="notice"><%= notice %></p>

<h1>Listing Cars</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Carold</th>
      <th>Carpic</th>
      <th>Carbrand</th>
      <th>Carmodel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.user_id %></td>
        <td><%= car.carold %></td>
        <td><%= car.carpic %></td>
        <td><%= car.carbrand %></td>
        <td><%= car.carmodel %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
