<p id="notice"><%= notice %></p>

<h1>Listing Bids</h1>

<table>
  <thead>
    <tr>
      <th>Shop</th>
      <th>Driver</th>
      <th>Go at</th>
      <th>Come back</th>
      <th>Pass1</th>
      <th>Pass2</th>
      <th>Pass3</th>
      <th>Pass4</th>
      <th>Cangodrive</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bids.each do |bid| %>
      <tr>
        <td><%= bid.shop_id %></td>
        <td><%= bid.driver_id %></td>
        <td><%= bid.go_at %></td>
        <td><%= bid.come_back %></td>
        <td><%= bid.pass1_id %></td>
        <td><%= bid.pass2_id %></td>
        <td><%= bid.pass3_id %></td>
        <td><%= bid.pass4_id %></td>
        <td><%= bid.cangodrive %></td>
        <td><%= link_to 'Show', bid %></td>
        <td><%= link_to 'Edit', edit_bid_path(bid) %></td>
        <td><%= link_to 'Destroy', bid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bid', new_bid_path %>
